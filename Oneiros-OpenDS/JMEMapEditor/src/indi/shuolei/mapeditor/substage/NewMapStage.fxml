<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<fx:root maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="250" prefWidth="300" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<children>
    	<TitledPane animated="true" collapsible="false" prefHeight="250" prefWidth="300" text="New Map">
         
        	<content>
         		<GridPane vgap="4.0">
            		<columnConstraints>
                		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
	                	<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
	              	</columnConstraints>
	              	<rowConstraints>
	                	<RowConstraints minHeight="10" prefHeight="15" vgrow="SOMETIMES" />
	                	<RowConstraints minHeight="10" prefHeight="15" vgrow="SOMETIMES" />
	                	<RowConstraints minHeight="10" prefHeight="25" vgrow="SOMETIMES" />
	              	</rowConstraints>
	               	<children>
	                	<Label prefHeight="15" prefWidth="80" text="Width: " GridPane.halignment="CENTER"></Label>
	                  	<Label prefHeight="15" prefWidth="80" text="Height: " GridPane.halignment="CENTER" GridPane.rowIndex="1"></Label>
	  
	                  	<TextField fx:id="mapWidth" prefHeight="30" prefWidth="50.0" GridPane.columnIndex="1" GridPane.valignment="CENTER">
	                     	<GridPane.margin>
	                        	<Insets right="30.0" />
	                     	</GridPane.margin>
	                  	</TextField>
	                  	<TextField fx:id="mapHeight" GridPane.columnIndex="1" GridPane.rowIndex="1">
	                     	<GridPane.margin>
	                        	<Insets right="30.0" />
	                     	</GridPane.margin>
	                  	</TextField>
	                  	<Button mnemonicParsing="true" onAction="#onNewMapAction" prefHeight="30" prefWidth="80" text="New" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER">                     
                  		</Button>
	                  
	                  
	               	</children>
               
            	</GridPane>
        	</content>
    	</TitledPane>
	</children>
</fx:root>
